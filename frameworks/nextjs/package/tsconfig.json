{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "target": "ES2018",
    "module": "ESNext",
    "lib": ["ES2018", "DOM"],
    "jsx": "react-jsx",
    "declaration": true,
    "declarationMap": true,
    "incremental": false,
    "outDir": "./dist",
    "rootDir": ".",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmitOnError": false,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "baseUrl": ".",
    "paths": {
      "@chaibuilder/sdk": ["../../../src/core/main/index.ts"],
      "@chaibuilder/sdk/server": ["../../../src/server/export.ts"],
      "@chaibuilder/sdk/pages": ["../../../src/pages/index.ts"],
      "@chaibuilder/sdk/render": ["../../../src/render/index.ts"],
      "@chaibuilder/sdk/ui": ["../../../src/ui/index.ts"],
      "@chaibuilder/sdk/tailwind": ["../../../src/tailwind/index.ts"],
      "@chaibuilder/sdk/web-blocks": ["../../../src/web-blocks/index.ts"],
      "@chaibuilder/sdk/runtime": ["../../../src/runtime.ts"],
      "@chaibuilder/sdk/supabase-actions": ["../../../src/express/actions/storage/index.ts"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "node_modules",
    "dist",
    "**/*.d.ts"
  ]
}
