{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": [
    "src/core/index.ts",
    "src/pages/index.ts",
    "src/render/index.ts",
    "src/runtime/index.ts",
    "src/web-blocks/index.ts",
    "src/utils/index.ts",
    "src/express/index.ts"
  ],
  "entryPointStrategy": "expand",
  "out": "docs/api",
  "plugin": ["typedoc-plugin-markdown"],
  "readme": "none",
  "hideGenerator": true,
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeExternals": true,
  "excludeInternal": true,
  "skipErrorChecking": true,
  "validation": {
    "notExported": false,
    "invalidLink": false,
    "notDocumented": false
  },
  "githubPages": false,
  "disableSources": false,
  "sourceLinkTemplate": "https://github.com/chaibuilder/sdk/blob/{gitRevision}/{path}#L{line}",
  "name": "ChaiBuilder SDK API Reference",
  "includeVersion": true,
  "sort": ["source-order"],
  "kindSortOrder": ["Function", "Class", "Interface", "TypeAlias", "Variable", "Enum"],
  "categorizeByGroup": true,
  "defaultCategory": "Other",
  "categoryOrder": ["Core", "Pages", "Render", "Runtime", "Web Blocks", "Utils", "Express", "*"]
}
