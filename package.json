{
  "name": "@chaibuilder/sdk",
  "description": "Open source low-code visual builder",
  "private": false,
  "author": "Suraj Air",
  "license": "BSD-3-Clause",
  "homepage": "https://chaibuilder.com",
  "version": "1.1.15",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/chaibuilder/sdk.git"
  },
  "files": [
    "dist"
  ],
  "main": "dist/core.js",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/core.js",
      "require": "./dist/core.cjs",
      "types": "./dist/core.d.ts"
    },
    "./render": {
      "import": "./dist/render.js",
      "require": "./dist/render.cjs",
      "types": "./dist/render.d.ts"
    },
    "./ui": {
      "import": "./dist/ui.js",
      "require": "./dist/ui.cjs",
      "types": "./dist/ui.d.ts"
    },
    "./lib": {
      "import": "./dist/lib.js",
      "require": "./dist/lib.cjs",
      "types": "./dist/lib.d.ts"
    },
    "./server": {
      "import": "./dist/server.js",
      "require": "./dist/server.cjs",
      "types": "./dist/server.d.ts"
    },
    "./studio": {
      "import": "./dist/studio.js",
      "require": "./dist/studio.cjs",
      "types": "./dist/studio.d.ts"
    },
    "./tailwind": {
      "import": "./dist/tailwind.js",
      "require": "./dist/tailwind.cjs",
      "types": "./dist/tailwind.d.ts"
    },
    "./styles": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "jest --colors --verbose",
    "test:watch": "jest --watch --colors",
    "test:coverage": "jest --coverage --colors",
    "e2e": "playwright test",
    "e2e:report": "playwright show-report",
    "e2e:ui": "playwright test --ui",
    "docs": "jsdoc -c jsdoc.conf.json",
    "prepare": "husky"
  },
