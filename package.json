{
  "name": "@chaibuilder/sdk",
  "description": "AI Enabled Open source visual builder",
  "private": false,
  "author": "Suraj Air",
  "license": "BSD-3-Clause",
  "homepage": "https://chaibuilder.com",
  "version": "2.2.28",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/chaibuilder/sdk.git"
  },
  "files": [
    "dist"
  ],
  "main": "dist/core.js",
  "sideEffects": false,
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "live": "tsc && vite build --config vite.config.live.ts",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest --run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "e2e": "playwright test",
    "e2e:report": "playwright show-report",
    "e2e:ui": "playwright test --ui",
    "prepare": "husky"
  },
  "exports": {
    ".": {
      "types": "./dist/core.d.ts",
      "import": "./dist/core.js",
      "require": "./dist/core.cjs"
    },
    "./render": {
      "types": "./dist/render.d.ts",
      "import": "./dist/render.js",
      "require": "./dist/render.cjs"
    },
    "./ui": {
      "types": "./dist/ui.d.ts",
      "import": "./dist/ui.js",
      "require": "./dist/ui.cjs"
    },
    "./tailwind": {
      "types": "./dist/tailwind.d.ts",
      "import": "./dist/tailwind.js",
      "require": "./dist/tailwind.cjs"
    },
    "./web-blocks": {
      "types": "./dist/web-blocks.d.ts",
      "import": "./dist/web-blocks.js",
      "require": "./dist/web-blocks.cjs"
    },
    "./runtime": {
      "types": "./dist/runtime.d.ts",
      "import": "./dist/runtime.js",
      "require": "./dist/runtime.cjs"
    },
    "./styles": {
      "import": "./dist/sdk.css",
      "require": "./dist/sdk.css"
    }
  },
  "peerDependencies": {
    "@chaibuilder/runtime": "2.2.0",
    "@types/react": "*",
    "@types/react-dom": "*",
    "jotai": "2.12.1",
    "react": "^18.0 || ^19.0",
    "react-dom": "^18.0 || ^19.0"
  },
