    "unsplash-js": "^7.0.18"
  },
  "devDependencies": {
    "@commitlint/cli": "17.7.1",
    "@commitlint/config-conventional": "17.7.0",
    "@emotion/react": "^11.11.1",
    "@faker-js/faker": "^8.3.1",
    "@nozbe/watermelondb": "^0.27.1",
    "@playwright/test": "^1.39.0",
    "@tanstack/eslint-plugin-query": "^5.8.4",
