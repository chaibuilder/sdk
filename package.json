{
  "name": "@chaibuilder/sdk",
  "description": "AI Enabled Open source website builder for ReactJS",
  "private": false,
  "author": "Suraj Air",
  "license": "BSD-3-Clause",
  "homepage": "https://chaibuilder.com",
  "version": "4.0.0-beta.6",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/chaibuilder/sdk.git"
  },
  "files": [
    "dist"
  ],
  "main": "dist/core.js",
  "sideEffects": false,
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "live": "tsc && vite build --config vite.config.live.ts",
    "live:serve": "cd live && npx http-server -p 8080",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:errors": "eslint . --ext ts,tsx --quiet --max-warnings 0",
    "lint:warnings": "eslint . --ext ts,tsx --min-warnings 1",
    "preview": "vite preview",
    "test": "vitest --run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "e2e": "playwright test",
    "e2e:report": "playwright show-report",
    "e2e:ui": "playwright test --ui",
    "docs:api": "typedoc",
    "docs:serve": "docsify serve docs",
    "prepare": "husky"
  },
  "exports": {
    ".": {
      "types": "./dist/core.d.ts",
      "import": "./dist/core.js",
      "require": "./dist/core.cjs"
    },
    "./pages": {
      "types": "./dist/pages.d.ts",
      "import": "./dist/pages.js",
      "require": "./dist/pages.cjs"
    },
    "./actions": {
      "types": "./dist/actions.d.ts",
      "import": "./dist/actions.js",
      "require": "./dist/actions.cjs"
    },
    "./render": {
      "types": "./dist/render.d.ts",
      "import": "./dist/render.js",
      "require": "./dist/render.cjs"
    },
    "./web-blocks": {
      "types": "./dist/web-blocks.d.ts",
      "import": "./dist/web-blocks.js",
      "require": "./dist/web-blocks.cjs"
    },
    "./runtime": {
      "types": "./dist/runtime.d.ts",
      "import": "./dist/runtime.js",
      "require": "./dist/runtime.cjs"
    },
    "./actions/supabase": {
      "types": "./dist/supabase-actions.d.ts",
      "import": "./dist/supabase-actions.js",
      "require": "./dist/supabase-actions.cjs"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "import": "./dist/utils.js",
      "require": "./dist/utils.cjs"
    },
    "./styles": {
      "import": "./dist/sdk.css",
      "require": "./dist/sdk.css"
    }
  },
